    <% @posts.each do |post| %>
        <%= link_to image_tag(post.image.url(:thumb)), post %></br>
        <%= post.body %></br>
        <!--</br><%= link_to 'Show', post %></br>-->
        
        <% if user_signed_in? %>
        <%= link_to 'Edit', edit_post_path(post) %></br>
        <%= link_to 'Destroy', post, method: :delete, data: { confirm: 'Are you sure?' } %></br>
        <%= link_to 'New Post', new_post_path %>
      
    <% end %>
 <% end %> 
  





